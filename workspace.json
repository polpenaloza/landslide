{
  "version": 2,
  "projects": {
    "sumbank": {
      "root": "apps/sumbank",
      "sourceRoot": "apps/sumbank/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/sumbank",
            "index": "apps/sumbank/src/index.html",
            "main": "apps/sumbank/src/main.tsx",
            "polyfills": "apps/sumbank/src/polyfills.ts",
            "tsConfig": "apps/sumbank/tsconfig.app.json",
            "assets": [
              "apps/sumbank/src/favicon.ico",
              "apps/sumbank/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sumbank/src/environments/environment.ts",
                  "with": "apps/sumbank/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "sumbank:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "sumbank:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sumbank/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/sumbank"
          ],
          "options": {
            "jestConfig": "apps/sumbank/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sumbank-e2e": {
      "root": "apps/sumbank-e2e",
      "sourceRoot": "apps/sumbank-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sumbank-e2e/cypress.json",
            "tsConfig": "apps/sumbank-e2e/tsconfig.e2e.json",
            "devServerTarget": "sumbank:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sumbank:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sumbank-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui-header": {
      "root": "libs/ui-header",
      "sourceRoot": "libs/ui-header/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-header/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/ui-header",
            "tsConfig": "libs/ui-header/tsconfig.lib.json",
            "project": "libs/ui-header/package.json",
            "entryFile": "libs/ui-header/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "react-is",
              "styled-components"
            ],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui-header"
          ],
          "options": {
            "jestConfig": "libs/ui-header/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "sumbank"
}